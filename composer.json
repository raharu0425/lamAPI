{
    "name": "lamAPI",
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.cakephp.org"
        }
    ],
    "require": {
        "cakephp/cakephp": "2.8.8"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "cakephp/debug_kit": "2.2.*",
        "cakedc/migrations": "dev-develop",
        "slywalker/boost_cake": "*",
        "josegonzalez/cakephp-entity": "dev-master"
    },
    "config": {
        "vendor-dir": "vendors/"
    },
    "extra": {
        "installer-paths": {
            "./plugins/{$name}/": [
                "cakephp/debug_kit",
                "cakedc/migrations",
                "slywalker/boost_cake",
                "josegonzalez/cakephp-entity"
            ]
        }
    }
}
